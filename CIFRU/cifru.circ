LIBRARY ieee;&#13;
USE ieee.std_logic_1164.all;&#13;
&#13;
ENTITY CU IS&#13;
  PORT (&#13;ok, reset: in std_logic;
        equal: in std_logic;
        clk: in std_logic;
        i_led, b_led :out std_logic; 
        load_1, load_2, load_3, load_4, load_5, load_6: out std_logic;
        reset_counter: out std_logic; 
        reset_registrii1: out std_logic;
        reset_registrii2: out std_logic
        
             );&#13;
END CU;&#13;

ARCHITECTURE Behavioral OF CU IS&#13;

&#13;type stare is (I,P1,D1,T1,L,P2,D2,T2,V);

signal current_state, next_state: stare := I;

BEGIN&#13;

process (clk,current_state, next_state) 

begin

if reset='1' then
current_state &lt;= I;

elsif rising_edge(clk) then
current_state &lt;= next_state;
end if;
end process;

process (clk, current_state, next_state) 
begin
case current_state is

when I=&gt;
reset_counter&lt;='1';
reset_registrii1&lt;='1';
reset_registrii2&lt;='1';
b_led&lt;='0';
i_led&lt;='0';
load_1&lt;='0';
load_2&lt;='0';
load_3&lt;='0';
load_4&lt;='0';
load_5&lt;='0';
load_6&lt;='0';


if ok='1' then
i_led&lt;='1';
next_state&lt;=P1;
reset_counter&lt;='1';
else
next_state&lt;=I;
end if;

when P1=&gt;
reset_registrii1&lt;='0';
reset_registrii2&lt;='0';
reset_counter&lt;='0';
load_1&lt;='1';
load_2&lt;='0';
load_3&lt;='0';
load_4&lt;='0';
load_5&lt;='0';
load_6&lt;='0';

if ok='1' then
next_state&lt;=D1;
reset_counter&lt;='1';
else
next_state&lt;=P1;
end if;

when D1=&gt;
reset_counter&lt;='0';
load_1&lt;='0';
load_2&lt;='1';
load_3&lt;='0';
load_4&lt;='0';
load_5&lt;='0';
load_6&lt;='0';
reset_registrii1&lt;='0';
reset_registrii2&lt;='0';
if ok='1' then
reset_counter&lt;='1';
next_state&lt;=T1;
else
next_state&lt;=D1;
end if;

when T1=&gt;
reset_counter&lt;='0';
load_1&lt;='0';
load_2&lt;='0';
load_3&lt;='1';
load_4&lt;='0';
load_5&lt;='0';
load_6&lt;='0';
reset_registrii1&lt;='0';
reset_registrii2&lt;='0';
if ok='1' then
next_state&lt;=L;
else
next_state&lt;=T1;
end if;

when L=&gt;
if ok='1' then
load_1&lt;='0';
load_2&lt;='0';
load_3&lt;='0';
load_4&lt;='0';
load_5&lt;='0';
load_6&lt;='0';
b_led&lt;='1';
i_led&lt;='0';
reset_registrii1&lt;='0';
reset_registrii2&lt;='0';
if ok='1' then
reset_counter&lt;='1';
next_state&lt;=P2;
else
next_state&lt;=L;
end if;
end if;

when P2=&gt;
reset_counter&lt;='0';
load_1&lt;='0';
load_2&lt;='0';
load_3&lt;='0';
load_4&lt;='1';
load_5&lt;='0';
load_6&lt;='0';
reset_registrii1&lt;='0';
reset_registrii2&lt;='0';
if ok='1' then
next_state&lt;=D2;
reset_counter&lt;='1';
else
next_state&lt;=P2;
end if;

when D2=&gt;
reset_counter&lt;='0';
load_1&lt;='0';
load_2&lt;='0';
load_3&lt;='0';
load_4&lt;='0';
load_5&lt;='1';
load_6&lt;='0';
reset_registrii1&lt;='0';
reset_registrii2&lt;='0';
if ok='1' then
reset_counter&lt;='1';
next_state&lt;=T2;
else
next_state&lt;=D2;
end if;

when T2=&gt;
reset_counter&lt;='0';
reset_registrii1&lt;='0';
reset_registrii2&lt;='0';
load_1&lt;='0';
load_2&lt;='0';
load_3&lt;='0';
load_4&lt;='0';
load_5&lt;='0';
load_6&lt;='1';
if ok='1' then
next_state&lt;=V;
else
next_state&lt;=T2;
end if;

when V=&gt;
if equal='0' then
load_1&lt;='0';
load_2&lt;='0';
load_3&lt;='0';
load_4&lt;='0';
load_5&lt;='0';
load_6&lt;='0';
next_state&lt;=L;
reset_registrii2&lt;='1';
else
next_state&lt;=I;
end if;

end case;
end process;
&#13; 

END Behavioral;&#13;
