  <vhdl name="registru">--REPEDE OANA
LIBRARY ieee;&#13;
USE ieee.std_logic_1164.all;&#13;
&#13;
ENTITY registru IS&#13;
  PORT (&#13; 
         load, clk, reset: in std_logic;
         data_in:  in std_logic_vector(3 downto 0);
         data_out: out std_logic_vector(3 downto 0)
    );&#13;
END registru;&#13;
&#13;
&#13;
ARCHITECTURE Behavioral OF registru IS&#13;
&#13;signal sig: std_logic_vector(3 downto 0):="0000";

BEGIN&#13;

&#13;process (clk, data_in, load)

	begin

		if reset='1' then
			sig&lt;="0000";
		elsif rising_edge(clk)then 
		if load ='1' then
		sig&lt;=data_in;
		end if;
		end if;
	end process;

data_out&lt;=sig;
&#13;
END Behavioral;&#13;
</vhdl>
  <vhdl name="COUNTER">--URECHE SIMONA
LIBRARY ieee;
USE ieee.std_logic_1164.all;
use ieee.std_logic_unsigned.all;

ENTITY COUNTER IS
  PORT (	clk,reset: in std_logic;
  		btn_up, btn_down: in std_logic;
  		
  		count: out std_logic_vector(3 downto 0)
    );
END COUNTER;

ARCHITECTURE Behavioral OF COUNTER IS
    signal value : std_logic_vector(3 downto 0) := "0000";
    signal cont_up, cont_down : std_logic := '0';
    
begin
    process(clk)
    begin
    if reset='1' then
    value&lt;="0000";
    
	
        elsif rising_edge(clk) then
            if btn_up = '1' then
                value &lt;= value + 1;
            end if;
            
            
            if btn_down = '1' then
                value &lt;= value - 1;
            end if;
        end if;
    
	   end process;
count &lt;= value;

END Behavioral;</vhdl>
